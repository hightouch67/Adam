             xmlns:local="clr-namespace:CMWPF"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Ressources/Style.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Converters/Converters.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Ressources/TextDictionaryFR.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
